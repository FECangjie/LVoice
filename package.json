  "scripts": {
    "node": "cross-env ENV=DEV node server.js",
    "dev": "cross-env ENV=DEV webpack-dev-server --config wp.config.js --inline --hot --progress --content-base pages/",
    "open": "cross-env ENV=DEV webpack-dev-server --config wp.config.js --inline --hot --quiet --progress --content-base pages/ --host 127.0.0.1 --port 8080 --open ",
    "build": "cross-env ENV=PROD CDN='../dist/' webpack --config wp.config.js",
     "start": "npm run node"
  },
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-0": "^6.16.0",
    "cross-env": "^3.1.4",
    "css-loader": "^0.26.1",
    "extract-loader": "^0.1.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "file-loader": "^0.11.1",
    "html-loader": "^0.4.5",
    "html-webpack-harddisk-plugin": "^0.0.2",
    "html-webpack-plugin": "^2.26.0",
    "less": "^2.7.2",
    "less-loader": "^4.0.4",
    "raw-loader": "^0.5.1",
    "style-loader": "^0.13.2",
    "url-loader": "^0.5.8",
    "webpack": "^2.6.0",
    "webpack-dev-server": "^2.4.5"
  }
